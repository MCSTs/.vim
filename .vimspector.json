{
  "configurations": {
    "run": {
      "adapter": "CodeLLDB",
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "N"
        }
      },
      "configuration": {
        "request": "launch",
        "protocol": "auto",
        "stopOnEntry": true,
        "console": "integratedTerminal",
        "program": "${fileDirname}/${fileBasenameNoExtension}",
        "cwd": "${workspaceRoot}"
      }
    }
  }
}

/*
    ${file}
    ${fileBasename}
    ${fileBasenameNoExtension}
    ${fileDirname}
    ${fileExtname}
    ${cwd}
*/
